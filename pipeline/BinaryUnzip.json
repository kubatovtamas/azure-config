{
	"name": "BinaryUnzip",
	"properties": {
		"activities": [
			{
				"name": "Unzip",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "test-container/bronze/FL_insurance_sample.csv.zip"
					},
					{
						"name": "Destination",
						"value": "test-container/bronze/unzipped_test.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"deleteFilesAfterCompletion": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "TriggeredZipFiles",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_ed4",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"triggerFileName": {
				"type": "string",
				"defaultValue": "@triggerBody().fileName"
			},
			"triggerFolderPath": {
				"type": "string",
				"defaultValue": "@triggerBody().folderPath"
			}
		},
		"annotations": []
	}
}